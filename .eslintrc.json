{
  "extends": ["next/core-web-vitals", "plugin:storybook/recommended"],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "args": "none"
      }
    ],
    "import/no-unresolved": "error",
    "import/named": "off",
    "no-console": "warn"
  },
  "import/resolver": {
    "typescript": {} // 이렇게 하면 tsconfig.json의 paths를 인식합니다
  }
}
